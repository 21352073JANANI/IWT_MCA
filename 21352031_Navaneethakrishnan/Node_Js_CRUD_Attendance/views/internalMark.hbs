<h3>EDIT STUDENT DETAILS</h3>

{{#if msg}}
<div class="alert">{{msg}}</div>
{{/if}}

{{#each rows}}
<form action="/internalMark/{{this.ID}}" method="post" autocomplete="false">
<div class = "form-group">
    <label for="name">NAME</label>
    <input type="text" autocomplete="off" name = "name" value="{{this.Name}}">
</div>

<div class = "form-group">
    <label for="registerNumber">REGISTER NUMBER</label>
    <input type="number" autocomplete="off" name = "registerNumber" value="{{this.RegisterNumber}}">
</div>

<div class = "form-group">
    <label for="institutionalMail">INSTITUTIONAL MAIL ID</label>
    <input type="mail" autocomplete="off" name = "institutionalMail" value="{{this.InstitutionalEmail}}">
</div>


<div class = "form-group">
    <label for="mobile">INTERNALS 1</label>
    <input type="number" autocomplete="off" name = "firstInternal" value="{{this.Internal_1}}">
</div>

<div class = "form-group">
    <label for="mobile">INTERNALS 2</label>
    <input type="number" autocomplete="off" name = "secondInternal" value="{{this.Internal_1}}">
</div>

<div class = "form-group">
    <label for="mobile">INTERNALS 3</label>
    <input type="number" autocomplete="off" name = "thirdInternal" value="{{this.Internal_1}}">
</div>



<div class = "form-group">
    <button type="submit">EVALUATE</button>
    <a class="viewAll" autocomplete="off" href="/">View All</a>
</div>
</form>
{{/each}}